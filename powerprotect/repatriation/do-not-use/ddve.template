            "Type": "String"
        },
        "DdveModel": {
            "Description": "  ",
            "Type": "String",
            "Default": "16TB-Model--m5.xlarge",
            "AllowedValues": [
                "16TB-Model--m5.xlarge",
                "32TB-Model--m5.2xlarge",
                "96TB-Model--m5.4xlarge",
                "256TB-Model--m5.8xlarge"
            ]
        },
